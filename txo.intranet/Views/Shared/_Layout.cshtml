<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">    
    
    <title>@ViewBag.Title - TXO Global</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/txofavicon.ico">
    
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <style>
        .container {
            padding-left: 0px;
            padding-right: 0px;
        }

        .btn-default {
            background-color:#d8761b;
            color: white;


        }
    </style>

</head>
<body>
    
    @*@Html.Partial("_LoginPartial")
    @RenderBody()
        *@
        

    <div id="wrapper">
        <div class="main-section">
            <div class="container">
                <!--Header Start-->
                <div class="header-sec">
                    <div class="header-left"><a href="#"><img src="~/Content/logo.png"></a></div>
                    <div class="header-right">
                        <ul class="reset">
                            <li>
                                <p>Call Us:<a href="tel:8663530841">866 353 0841</a></p>
                            </li>
                            <li class="search">
                                <input type="text" class="searchtxt" value="Search">
                                <input type="" class="button" value="">
                            </li>
                            <li>
                            </li>
                            <li>@Html.ActionLink("Login", "Login", "Account", routeValues: null, htmlAttributes: new { @class="login" })</li>                            
                        </ul>
                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                </div>
                <!--Navigation Start-->
                <div class="navigation-sec">
                    <ul class="reset">
                        <li><a href="#">Home</a></li>
                        <li><a href="#">Supliers</a></li>
                        <li><a href="#">Support</a></li>
                        <li><a href="#">Contact Us</a></li>
                        <li><a href="#">About</a></li>
                    </ul>
                    <div class="clear"></div>
                </div>
                <!--Login-start-->
                <div class="inner-banner">
                    <img src="~/Content/contact-bannerimg.jpg">
                    <div class="container">
                        <div class="inner-caption">
                            <h3>@ViewBag.Title</h3>
                        </div>
                    </div>
                </div>

                @RenderBody()

            </div>
        </div>
    </div>
    <!--Footer Copyright Start-->
    <div class="copy-right">
        <div class="container" style="padding:18px 0px 18px 0px; background-color:#626365;">
            <div class="copy-lf">
                <p><img src="./Support Request_files/copy-logo.png"> © 2016 <span><strong>TXO Global.</strong></span> All Rights Reserved. | <a href="http://www.txoglobal.com/PrivacyPolicy">Privacy Policy</a> | <a href="http://www.txoglobal.com/About">About</a> | <a href="http://www.txoglobal.com/SupportCenter">Support</a></p>
            </div>
            <div class="social-icon">
                <ul class="reset">
                    <li><a href="http://www.txoglobal.com/SupportRequest#" class="fb"></a></li>
                    <li><a href="http://www.txoglobal.com/SupportRequest#" class="in"></a></li>
                    <li><a href="http://www.txoglobal.com/SupportRequest#" class="twt"></a></li>
                    <li><a href="http://www.txoglobal.com/SupportRequest#" class="utube"></a></li>
                </ul>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
  

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
